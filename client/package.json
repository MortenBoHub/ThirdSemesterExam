    "class-variance-authority": "^0.7.1",
    "daisyui": "^5.1.12",
    "dotnet-json-refs": "^1.0.0",
    "jotai": "^2.14.0",
    "jotai-devtools": "^0.5.3",
    "lucide-react": "^0.556.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hot-toast": "^2.6.0",
